    <title>测试图片显示</title>
    <url>/2024/02/04/%E6%B5%8B%E8%AF%95%E5%9B%BE%E7%89%87%E6%98%BE%E7%A4%BA/</url>
    <content><![CDATA[<p>以下测试图片显示功能</p>
<img data-src="./../images/image-20240204092451201.png" alt="image-20240204092451201" style="zoom: 67%;" />

<figure class="highlight c#"><table><tr><td class="code"><pre><span class="line"><span class="comment"><span class="doctag">///</span> <span class="doctag">&lt;summary&gt;</span></span></span><br><span class="line"><span class="comment"><span class="doctag">///</span> 接收调拨入库单</span></span><br><span class="line"><span class="comment"><span class="doctag">///</span> <span class="doctag">&lt;/summary&gt;</span></span></span><br><span class="line"><span class="comment"><span class="doctag">///</span> <span class="doctag">&lt;param name=&quot;req_dto&quot;&gt;</span>请求参数集合<span class="doctag">&lt;/param&gt;</span></span></span><br><span class="line"><span class="comment"><span class="doctag">///</span> <span class="doctag">&lt;returns&gt;</span><span class="doctag">&lt;/returns&gt;</span></span></span><br><span class="line"><span class="keyword">public</span> <span class="keyword">async</span> Task&lt;MessageModel&lt;<span class="built_in">string</span>&gt;&gt; TransferStorage(Req_TransferStorageDTO req_dto)</span><br><span class="line">&#123;</span><br><span class="line">    <span class="comment">// 信息记录集合</span></span><br><span class="line">    List&lt;me_ply_Record&gt; records = <span class="keyword">new</span> List&lt;me_ply_Record&gt;();</span><br><span class="line">    <span class="comment">// 请求唯一标识</span></span><br><span class="line">    <span class="built_in">string</span> logID = Guid.NewGuid().ToString(<span class="string">&quot;N&quot;</span>);</span><br><span class="line">    <span class="comment">// 获取ip地址</span></span><br><span class="line">    <span class="built_in">string</span> ipaddress = _httpContextAccessor.HttpContext.Connection.RemoteIpAddress.ToString();</span><br><span class="line">    <span class="comment">// 保存记录信息</span></span><br><span class="line">    <span class="keyword">var</span> <span class="keyword">record</span> = <span class="keyword">new</span> me_ply_Record</span><br><span class="line">    &#123;</span><br><span class="line">        cInterFaceName = <span class="string">&quot;TransferStorage&quot;</span>,</span><br><span class="line">        cDirection = <span class="string">&quot;SAP→前处理系统&quot;</span>,</span><br><span class="line">        cIpAddress = ipaddress,</span><br><span class="line">        cIpRegion = IPUtil.ClientRegion(ipaddress),</span><br><span class="line">        bIsSuccess = <span class="literal">false</span>,</span><br><span class="line">        ILOGID = logID</span><br><span class="line">    &#125;;</span><br><span class="line"></span><br><span class="line">    MessageModel&lt;<span class="built_in">string</span>&gt; message = <span class="keyword">new</span> MessageModel&lt;<span class="built_in">string</span>&gt;();</span><br><span class="line"></span><br><span class="line">    <span class="keyword">var</span> url = UrlAddress.Pretreatment_PLY;</span><br><span class="line">    <span class="keyword">try</span></span><br><span class="line">    &#123;</span><br><span class="line">        <span class="comment">// 接收的报文</span></span><br><span class="line">        <span class="keyword">record</span>.cJson = req_dto.ToJson();</span><br><span class="line"></span><br><span class="line">        <span class="comment">//最终报文</span></span><br><span class="line"></span><br><span class="line">        <span class="meta">#<span class="keyword">region</span> 在前处理系统拿到工厂信息</span></span><br><span class="line">        <span class="keyword">var</span> factory = <span class="keyword">await</span> RequestAPI.DoGet_V2(url, <span class="string">&quot;Pretreatment_PLY&quot;</span>, <span class="string">&quot;/api/pem/factory/get&quot;</span>);</span><br><span class="line">        <span class="keyword">if</span> (factory.Code != <span class="number">200</span>)</span><br><span class="line">        &#123;</span><br><span class="line">            <span class="keyword">return</span> MessageError.Error(<span class="string">&quot;获取前处理系统工厂失败.请联系管理员&quot;</span>);</span><br><span class="line">        &#125;</span><br><span class="line"></span><br><span class="line">        <span class="keyword">var</span> factory_obj = JToken.Parse(factory.Response);</span><br><span class="line">        <span class="keyword">if</span> (factory_obj <span class="keyword">is</span> <span class="literal">null</span>)</span><br><span class="line">        &#123;</span><br><span class="line">            <span class="keyword">return</span> MessageError.Error(<span class="string">&quot;获取前处理系统工厂失败.请联系管理员&quot;</span>);</span><br><span class="line">        &#125;</span><br><span class="line"></span><br><span class="line">        <span class="keyword">var</span> factory_data = factory_obj.SelectToken(<span class="string">&quot;data&quot;</span>).ToList();</span><br><span class="line">        <span class="meta">#<span class="keyword">endregion</span></span></span><br><span class="line"></span><br><span class="line">        <span class="meta">#<span class="keyword">region</span> 在前处理系统拿到仓库信息</span></span><br><span class="line">        <span class="keyword">var</span> req_store = <span class="keyword">new</span></span><br><span class="line">        &#123;</span><br><span class="line">            code = <span class="string">&quot;&quot;</span>,</span><br><span class="line">            name = <span class="string">&quot;&quot;</span>,</span><br><span class="line">            factoryId = <span class="number">0</span>,</span><br><span class="line">            pageIndex = <span class="number">0</span>,</span><br><span class="line">            pageSize = <span class="number">0</span>,</span><br><span class="line">            isGetTotalCount = <span class="literal">true</span></span><br><span class="line">        &#125;;</span><br><span class="line">        <span class="comment">//将对象序列化为json</span></span><br><span class="line">        <span class="keyword">var</span> req_store_json = req_store.ToJson();</span><br><span class="line">        <span class="keyword">var</span> store = <span class="keyword">await</span> RequestAPI.DoPost_V2(url, <span class="string">&quot;Pretreatment_PLY&quot;</span>, <span class="string">&quot;/api/wms/store/getstores&quot;</span>, req_store_json);</span><br><span class="line">        <span class="keyword">if</span> (store.Code != <span class="number">200</span>)</span><br><span class="line">        &#123;</span><br><span class="line">            <span class="keyword">return</span> MessageError.Error(<span class="string">&quot;获取前处理系统仓库失败.请联系管理员&quot;</span>);</span><br><span class="line">        &#125;</span><br><span class="line"></span><br><span class="line">        <span class="keyword">var</span> store_obj = JToken.Parse(store.Response);</span><br><span class="line">        <span class="keyword">if</span> (store_obj <span class="keyword">is</span> <span class="literal">null</span>)</span><br><span class="line">        &#123;</span><br><span class="line">            <span class="keyword">return</span> MessageError.Error(<span class="string">&quot;获取前处理系统仓库失败.请联系管理员&quot;</span>);</span><br><span class="line">        &#125;</span><br><span class="line"></span><br><span class="line">        <span class="keyword">var</span> store_data = store_obj.SelectToken(<span class="string">&quot;data.data&quot;</span>).ToList();</span><br><span class="line">        <span class="meta">#<span class="keyword">endregion</span></span></span><br><span class="line"></span><br><span class="line"></span><br><span class="line">        <span class="keyword">var</span> this_factory_data = factory_data.Where(q =&gt; q.SelectToken(<span class="string">&quot;code&quot;</span>).ToString() == req_dto.putawayFactoryCode).FirstOrDefault();</span><br><span class="line">        <span class="keyword">if</span> (this_factory_data <span class="keyword">is</span> <span class="literal">null</span>)</span><br><span class="line">        &#123;</span><br><span class="line">            <span class="keyword">return</span> message = MessageError.Error(<span class="number">204</span>, <span class="string">$&quot;工厂编码 [<span class="subst">&#123;req_dto.putawayFactoryCode&#125;</span>] 在前处理系统中不存在&quot;</span>);</span><br><span class="line">        &#125;</span><br><span class="line"></span><br><span class="line">        <span class="keyword">var</span> this_store_data = store_data.Where(q =&gt; q.SelectToken(<span class="string">&quot;code&quot;</span>).ToString() == req_dto.putawayStoreCode).FirstOrDefault();</span><br><span class="line">        <span class="keyword">if</span> (this_store_data <span class="keyword">is</span> <span class="literal">null</span>)</span><br><span class="line">        &#123;</span><br><span class="line">            <span class="keyword">return</span> message = MessageError.Error(<span class="number">204</span>, <span class="string">$&quot;仓库编码 [<span class="subst">&#123;req_dto.putawayStoreCode&#125;</span>] 在前处理系统中不存在&quot;</span>);</span><br><span class="line">        &#125;</span><br><span class="line"></span><br><span class="line">        <span class="keyword">var</span> order_mes = _mapper.Map&lt;PLY_TransferStorageDTO&gt;(req_dto);</span><br><span class="line"></span><br><span class="line">        <span class="comment">//子表</span></span><br><span class="line">        <span class="comment">//到前处理系统查询四个日期</span></span><br><span class="line">        <span class="comment">//...</span></span><br><span class="line"></span><br><span class="line">        <span class="comment">//写入前处理系统</span></span><br><span class="line">        <span class="comment">//日志相关操作</span></span><br><span class="line">        <span class="comment">//...</span></span><br><span class="line"></span><br><span class="line"></span><br><span class="line"></span><br><span class="line"></span><br><span class="line">        _log.Info(<span class="string">$&quot;提交调拨入库单到前处理系统成功,数据:<span class="subst">&#123;req_dto.ToJson()&#125;</span>&quot;</span>);</span><br><span class="line">        <span class="keyword">record</span>.bIsSuccess = <span class="literal">true</span>;</span><br><span class="line">        message = MessageSuccess.Success(<span class="string">&quot;推送成功&quot;</span>);</span><br><span class="line">    &#125;</span><br><span class="line">    <span class="keyword">catch</span> (Exception ex)</span><br><span class="line">    &#123;</span><br><span class="line">        _log.Error(<span class="string">$&quot;接收调拨入库单错误:<span class="subst">&#123;ex&#125;</span>&quot;</span>);</span><br><span class="line">        <span class="keyword">return</span> MessageError.Error(<span class="string">&quot;操作失败:请联系管理人员&quot;</span>);</span><br><span class="line">    &#125;</span><br><span class="line">    <span class="keyword">finally</span></span><br><span class="line">    &#123;</span><br><span class="line">        records.Add(<span class="keyword">record</span>);</span><br><span class="line">        <span class="keyword">await</span> _record.Add(records);</span><br><span class="line">    &#125;</span><br><span class="line"></span><br><span class="line">    <span class="keyword">return</span> message;</span><br><span class="line">&#125;</span><br></pre></td></tr></table></figure>

]]></content>
